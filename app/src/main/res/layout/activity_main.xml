<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/buttonPercent"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="240dp"
        android:width="@android:dimen/app_icon_size"
        android:text="%"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button0" />

    <Button
        android:id="@+id/buttonHooks"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="240dp"
        android:width="@android:dimen/app_icon_size"
        android:text="()"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonE" />

    <Button
        android:id="@+id/buttonC"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="240dp"
        android:width="@android:dimen/app_icon_size"
        android:text="C"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonLG" />

    <Button
        android:id="@+id/buttonPI"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="240dp"
        android:width="@android:dimen/app_icon_size"
        android:text="𝝅"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonDot" />

    <Button
        android:id="@+id/buttonCA"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="240dp"
        android:width="@android:dimen/app_icon_size"
        android:text="CA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buttonSquared2"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="192dp"
        android:width="@android:dimen/app_icon_size"
        android:text="x^2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonE" />

    <Button
        android:id="@+id/buttonDivide"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="192dp"
        android:width="@android:dimen/app_icon_size"
        android:text="/"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonDot" />

    <Button
        android:id="@+id/buttonSin"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="192dp"
        android:width="@android:dimen/app_icon_size"
        android:text="sin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buttonSquaredByY"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="192dp"
        android:width="@android:dimen/app_icon_size"
        android:text="x^y"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button0" />

    <Button
        android:id="@+id/buttonSquareRoot"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="192dp"
        android:width="@android:dimen/app_icon_size"
        android:text=" √x"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonLG" />

    <Button
        android:id="@+id/button8"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="144dp"
        android:width="@android:dimen/app_icon_size"
        android:text="8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonE" />

    <Button
        android:id="@+id/buttonMultiply"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="144dp"
        android:width="@android:dimen/app_icon_size"
        android:text="*"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonDot" />

    <Button
        android:id="@+id/buttonCos"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="144dp"
        android:width="@android:dimen/app_icon_size"
        android:text="cos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button9"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="144dp"
        android:width="@android:dimen/app_icon_size"
        android:text="9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button0" />

    <Button
        android:id="@+id/button7"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="144dp"
        android:width="@android:dimen/app_icon_size"
        android:text="7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonLG" />

    <Button
        android:id="@+id/button5"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="96dp"
        android:width="@android:dimen/app_icon_size"
        android:text="5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonE" />

    <Button
        android:id="@+id/buttonSubtract"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="96dp"
        android:width="@android:dimen/app_icon_size"
        android:text="-"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonDot" />

    <Button
        android:id="@+id/buttonTan"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="96dp"
        android:width="@android:dimen/app_icon_size"
        android:text="tan"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button6"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="96dp"
        android:width="@android:dimen/app_icon_size"
        android:text="6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button0" />

    <Button
        android:id="@+id/button4"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="96dp"
        android:width="@android:dimen/app_icon_size"
        android:text="4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonLG" />

    <Button
        android:id="@+id/button1"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        android:width="@android:dimen/app_icon_size"
        android:text="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonLG" />

    <Button
        android:id="@+id/button2"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        android:width="@android:dimen/app_icon_size"
        android:text="2"
        android:onClick="AddStringToTextView[Addable]"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonE"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/buttonLN"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        android:width="@android:dimen/app_icon_size"
        android:text="ln"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        android:width="@android:dimen/app_icon_size"
        android:text="3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button0" />

    <Button
        android:id="@+id/buttonPlus"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        android:width="@android:dimen/app_icon_size"
        android:text="+"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonDot" />

    <Button
        android:id="@+id/buttonLG"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:width="@android:dimen/app_icon_size"
        android:text="lg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buttonE"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:width="@android:dimen/app_icon_size"
        android:text="e"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonLG" />

    <Button
        android:id="@+id/button0"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:width="@android:dimen/app_icon_size"
        android:text="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonE" />

    <Button
        android:id="@+id/buttonDot"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:width="@android:dimen/app_icon_size"
        android:text="."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button0" />

    <Button
        android:id="@+id/buttonEqual"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:width="@android:dimen/app_icon_size"
        android:text="="
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonDot" />

    <TextView
        android:id="@+id/textViewCurrentAction"
        android:layout_width="344dp"
        android:layout_height="64dp"
        android:fontFamily="@font/aldrich"
        android:gravity="right"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonPercent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewPreviousAction"
        android:gravity="right"
        android:layout_width="275dp"
        android:layout_height="36dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="24dp"
        android:fontFamily="@font/aldrich"
        app:layout_constraintBottom_toTopOf="@+id/textViewCurrentAction"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>